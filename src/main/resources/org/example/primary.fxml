<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TabPane?>

<?import javafx.scene.control.Tab?>

<TabPane fx:id="tabPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PrimaryController">
   <tabs>
      <Tab fx:id="inputTab">
         <fx:include fx:id="inputPage" source="inputPage.fxml"/>
      </Tab>
      <Tab fx:id="resultTab">
         <fx:include fx:id="resultPage" source="resultPage.fxml"/>
      </Tab>
   </tabs>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</TabPane>
